
MARKING {
  m: stack(succ(succ(succ(succ(zero)))), empty)
  n: succ(succ(succ(zero)))
}

TRANSITION case1 {
  MATCH {
    m: stack(zero, Stack)
    n: Y
  }
  PRODUCE {
    m: Stack
    n: succ(Y)
  }
}

TRANSITION case2 {
  MATCH {
    m: stack(succ(X), Stack)
    n: zero
  }
  PRODUCE {
    m: stack(X, Stack)
    n: succ(zero)
  }
}

TRANSITION case3 {
  MATCH {
    m: stack(succ(X), Stack)
    n: succ(Y)
  }
  PRODUCE {
    m: stack(succ(X), stack(X, Stack))
    n: Y
  }
}