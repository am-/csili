INTERFACE {
  INPUT {
    input
  }
  OUTPUT {
    output
  }
}

MARKING {
  reversed: nil
}

TRANSITION start {
  MATCH {
    input: List
  }
  PRODUCE {
    original: List
  }
}

TRANSITION addElement {
  MATCH {
    original: cons(Head, Tail)
    reversed: ReversedList
  }
  PRODUCE {
    original: Tail
    reversed: cons(Head, ReversedList)
  }
}

TRANSITION return {
  MATCH {
    original: nil
    reversed: ReversedList
  }
  PRODUCE {
    reversed: nil
    output: ReversedList
  }
}
